// Create a function that takes a vector (array) of strings. Return all words in the vector (array) that are exactly four letters.



#include <iostream>
using namespace std;
int main() {
    string a;
    string str[] = {"Kangaroo", "Bear", "Fox","Nile"};
    int len = sizeof(str)/sizeof(str[0]);
    for(int i=0;i<len;i++){
        a = str[i];
        if(a.length()==4){
            cout<<a<<endl;
        }
    }
    return 0;
}
