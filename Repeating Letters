// Create a function that takes a string and returns a string in which each character is repeated once.

#include <iostream>
using namespace std;
void doubleChar(string str){
    for(int i=0;i<str.length();i++){
        cout<<str[i]<<str[i];
    }
}
int main() {
    string str;
    cout<<"Enter A String :- ";
    getline(cin,str);
    doubleChar(str);
    return 0;
}
